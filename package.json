{
  "name": "styles-library",
  "version": "1.0.0",
  "description": "A collection of reusable Sass styles",
  "main": "index.js",
  "scripts": {
    "watch": "gulp",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:css:fix": "stylelint --fix 'src/**/*.scss'",
    "format:css": "prettier --write 'src/**/*.scss'",
    "commit": "cz"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "sass",
    "styles",
    "library"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "gitmoji-cli": "^8.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-notify": "^4.0.0",
    "gulp-sass": "^5.1.0",
    "prettier": "^2.8.6",
    "sass": "^1.59.3",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.5.0"
  }
